{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootbox.js@6.0.0",
      "destination": "wwwroot/bootbox.js/",
      "files": [
        "bootbox.min.js"
      ]
    },
    {
      "library": "animate.css@4.1.1",
      "destination": "wwwroot/animate.css/",
